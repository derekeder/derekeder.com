---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title>derekeder.com/blog</title>
  <link href="http://derekeder.com/blog/"/>
  <link type="application/atom+xml" rel="self" href="http://derekeder.com/blog/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://derekeder.com/blog/</id>
  <author>
    <name>Derek Eder's Blog</name>
    <email>derek.eder@gmail.com</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <id>http://derekeder.com/blog{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="http://derekeder.com/blog{{ post.url }}"/>
    <title>{{ post.title }}</title>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>Derek Eder</name>
      <uri>http://derekeder.com/blog/</uri>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
 
</feed>